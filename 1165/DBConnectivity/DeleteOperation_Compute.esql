

CREATE COMPUTE MODULE DeleteOperation_Compute
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		DECLARE EMPID CHARACTER;
		
		SET EMPID = InputLocalEnvironment.HTTP.Input.QueryString.empid;
		
		PASSTHRU 'DELETE FROM empdetails WHERE EMPID = ?' TO DataSource.DBDSN VALUES(EMPID);
		
		SET OutputRoot.XMLNSC.MESSAGE = 'DATA DELETED SUCCESSFULLY';
	END;
END MODULE;
